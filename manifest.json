{
"manifest_version": 2,
  "name": "Pixiv Fast",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "*://*.pixiv.net/member_illust.php?mode=medium&illust_id=*",
        "*://*.pixiv.net/bookmark.php*"
      ],
      "js": ["thirdParty/jquery-2.2.0.min.js", "settings.js","content.js"]
    }
  ],


  "options_page": "options.html",
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "permissions": [
    "*://*.pixiv.net/*",
    "storage"
  ]
}